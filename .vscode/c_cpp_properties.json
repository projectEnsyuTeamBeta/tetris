{
  "configurations": [
    {
      "name": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "__evenaccess=",
        "__near="
      ],
      "compilerPath": "gcc",
      "intelliSenseMode": "windows-gcc-x64",
      "cStandard": "${default}",
      "cppStandard": "${default}"
    },
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/**",
        "C:/Vc7/include"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "__evenaccess=",
        "__near="
      ],
      "compilerPath": "C:/Vc7/bin/cl.exe",
      "intelliSenseMode": "windows-msvc-x86",
      "cStandard": "c17",
      "cppStandard": "c++17"
    }
  ],
  "version": 4
}